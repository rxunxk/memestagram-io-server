{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/*",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "index.js"
    },
    {
      "src": "/users",
      "dest": "index.js"
    },
    {
      "src": "/auth",
      "dest": "index.js"
    },
    {
      "src": "/posts",
      "dest": "index.js"
    },
    {
      "src": "/posts/(.*)",
      "dest": "index.js"
    }
  ]
}
